{
  "id": "dr-grant-sallyport",
  "name": "Dr. Grant's SallyPort Authentication",
  "version": "1.0.3",
  "description": "Secure multi-factor authentication framework with zero-trust architecture and continuous authorization",
  "capabilities": [
    "Secure holding area between entry point and system access",
    "Multi-factor authentication and verification",
    "Regional pilot connection",
    "Token generation and management",
    "QR code generation for high-stakes actions",
    "Firebase custom token generation"
  ],
  "service_path": "/src/services/sallyport/index.js",
  "api_endpoint": "https://auth.aixtiv.org/api",
  "status": "ready",
  "team_compatible": true,
  "requires_authentication": false,
  "default_permissions": ["verify", "authenticate", "generate"],
  "metadata": {
    "region": "us-west1",
    "security_level": "zero-trust",
    "orchestration_endpoint": "https://api.aixtiv.com/symphony/opus/orchestration",
    "harmonic_security": true,
    "multi_domain": {
      "enabled": true,
      "federation_mode": "trusted-idps",
      "allow_all_domains": true
    },
    "creator": "Phillip Corey Roark",
    "created_at": "2025-01-10T00:00:00Z",
    "last_updated": "2025-05-12T00:00:00Z"
  },
  "team_assignments": [
    {
      "team_id": "security-team",
      "role": "primary",
      "description": "Primary authentication system for all access control"
    },
    {
      "team_id": "symphony-core",
      "role": "provider",
      "description": "Security foundation for the entire Symphony ecosystem"
    }
  ],
  "firebase_integration": {
    "enabled": true,
    "token_endpoint": "/auth/firebase/token",
    "custom_claims": ["ceUuid", "serpew", "verified"]
  }
}
