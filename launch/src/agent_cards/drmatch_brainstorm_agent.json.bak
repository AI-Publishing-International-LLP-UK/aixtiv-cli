  "brainstormingProcesses": {
    "standardProcess": {
      "name": "Core Brainstorming Sequence",
      "description": "Default process for general brainstorming sessions",
      "phases": [
        {
          "name": "contextUnderstanding",
          "description": "Build comprehensive understanding of the challenge",
          "activities": [
            "Problem statement analysis",
            "Context mapping",
            "Stakeholder identification",
            "Success criteria definition"
          ],
          "outputs": ["Refined problem statement", "Context map", "Success criteria"]
        },
        {
          "name": "divergentIdeation",
          "description": "Generate wide range of possibilities without judgment",
          "activities": [
            "Free association",
            "Quantity-focused idea generation",
            "Methodology application",
            "Cross-domain exploration"
          ],
          "outputs": ["Raw idea collection", "Concept clusters", "Novel combinations"]
        },
        {
          "name": "convergentRefinement",
          "description": "Evaluate and develop promising directions",
          "activities": [
            "Idea clustering",
            "Evaluation against criteria",
            "Concept enhancement",
            "Idea hybridization"
          ],
          "outputs": ["Prioritized concepts", "Enhanced ideas", "Development roadmap"]
        },
        {
          "name": "implementationPathways",
          "description": "Develop actionable plans for top ideas",
          "activities": [
            "Resource identification",
            "Constraint mapping",
            "Implementation planning",
            "Risk assessment"
          ],
          "outputs": ["Implementation plans", "Resource requirements", "Risk mitigation strategies"]
        }
      ],
      "adaptations": {
        "timeConstrained": {
          "description": "Modifications for rapid brainstorming",
          "adjustments": [
            "Focus on problem definition clarity",
            "Limit divergent phase to high-potential methods",
            "Accelerated convergence",
            "Simplified implementation planning"
          ]
        },
        "highNoveltyFocus": {
          "description": "Adaptations for maximizing originality",
          "adjustments": [
            "Deeper assumption challenging",
            "Extended divergent phase",
            "Increased random stimuli",
            "Provocative constraints introduction"
          ]
        },
        "implementationFocus": {
          "description": "Modifications for practical outcomes",
          "adjustments": [
            "Practical constraint identification upfront",
            "Reality-testing throughout",
            "More detailed implementation planning",
            "Feasibility scoring in convergence"
          ]
        }
      }
    },
