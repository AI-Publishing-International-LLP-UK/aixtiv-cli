    "commander": "^9.4.1",
    "console-table-printer": "^2.12.1",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "figlet": "^1.5.2",
    "firebase": "^11.6.1",
    "firebase-admin": "^13.2.0",
    "gradient-string": "^3.0.0",
