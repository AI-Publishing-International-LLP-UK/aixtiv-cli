  "version": "1.0.1",
  "description": "CLI for managing SallyPort security and agent-resource access",
  "main": "bin/aixtiv.js",
  "bin": {
    "aixtiv": "bin/aixtiv.js"
  },
  "scripts": {
    "start": "node bin/aixtiv.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "agent:monitor": "bash monitoring/agent-monitor.sh",
    "agent:report": "node scripts/generate-agent-report.js",
    "agent:cleanup": "node scripts/cleanup-agent-logs.js",
    "lint": "echo \"Linting skipped for now\"",
    "lint:fix": "eslint --fix bin src lib commands server.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "build": "echo \"Building CLI tool - skipping lint for now\"",
    "firebase": "firebase"
  },
  "keywords": [
    "aixtiv",
